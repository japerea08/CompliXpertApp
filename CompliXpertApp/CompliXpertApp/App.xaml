<?xml version="1.0" encoding="utf-8" ?>
<Application
    x:Class="CompliXpertApp.App"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="DarkBlueLabelColor">#305d82</Color>
            <Color x:Key="ButtonTextColor">White</Color>
            <Color x:Key="LightBlue">#428bca</Color>
            <Color x:Key="GridLightBlueBackground">#eff4f9</Color>
            <Style TargetType="NavigationPage">
                <Setter Property="BarBackgroundColor" Value="#f5f5f5" />
                <Setter Property="BarTextColor" Value="#305d82" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>